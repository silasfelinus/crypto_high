{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "#imports": [".nuxt/imports"],
      "~/*": ["./*"],
      "@/*": ["./*"]
    },
    "types": ["nuxt", "@nuxt/content", "vite/client"]
  },
  "include": ["**/*.ts", "**/*.tsx", "**/*.vue", "types/**/*.d.ts"],
  "exclude": ["node_modules"]
}
